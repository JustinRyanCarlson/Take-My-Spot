<div class="container">
    <h3 class="text-center">Rental List</h3>
    <hr>
    <div class="row">
        {{#each Owners}}
        <div class="col-sm-4">
            <div class="panel panel-default">
                <div class="panel-body">
                    <h5 class="text-center">Address: {{this.revAddress}}, {{this.revCity}} {{this.revZipcode}}</h5>
                    <h5 class="text-center">Rental Date (yyyy-mm-dd): {{this.date}}</h5>
                    <h5 class="text-center">Price: ${{this.revPrice}}/day</h5>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
    <br>
    <div class="row">
        <div class="col-sm-12">
            <div class="text-center">
                <button class="btn btn-primary" id="submit-button" data-toggle="modal" onClick="window.location='/renter'" ;>Rent More Properties</button>
            </div>
        </div>
    </div>
</div>
